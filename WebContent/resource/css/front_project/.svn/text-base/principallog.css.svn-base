.personal {
	min-height: 74%;

	box-sizing: border-box;
}
.personal>div {
	box-sizing: border-box;
	padding: 14px 0 11px;
}
.personal .autstate {
	width: 100%;
	border-bottom: 1px solid #C0C0C0;
	font:bold 18px sans-serif;
}
.personal .detail>div,.personal .detail>ul {
	float: left;
}
.personal .detailright{
	width: 1000px;
	margin-left: 15px;
	padding-top: 10px;
	margin-bottom: 20px;
	box-sizing: border-box;
	padding: 15px;
	border-radius: 15px;
	font-size: 14px;
}
.personal .detailright input,.personal .detailright select {
	height: 20px;
	width: 125px;
	box-sizing: border-box;
	border-radius: 5px;
	border: 1px solid lightslategray;
}
.personal .detailright table{
	width: 100%;
}
.personal .detailright tr {
	height: 30px;
}
.personal .detailright input {
	box-sizing: border-box;
	padding: 2px 4px;
}
.detailright .derighttop div{
	float: left;
	margin-left: 20px;
	margin-top: 15px;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
}
.detailright .derightbottom{
	margin-top: 20px;
}
.detailright .derightbottom table{
	background-color: #999;
}
.detailright .derightbottom th,.detailright .derightbottom td{
	text-align: center;
	font-weight: normal;
	background-color: white;
}
.detailright .derightbottom caption{
	font-size: 18px;
	height: 30px;
}
.detailright .derightbottom th{
	background-color: #4488FF;
	color: white;
}
.detailright .tabfooter {
	margin-top: 10px;
	text-align: center;
}
.detailright .tabfooter button{
	background-color: transparent;
	border: 0 none;
	cursor: pointer;
}
