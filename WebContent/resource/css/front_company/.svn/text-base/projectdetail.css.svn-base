.personal {
	min-height: 74%;
	box-sizing: border-box;
}
.personal .operate {
	margin:20px 0;
}
.personal .operate div {
	float:left;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 100px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-right: 20px;
}
.personal .intitle{
	font-size: 22px;
	padding: 10px;
}
.personal .intitle span{
	font-size: 16px;
	margin-left: 20px;
}
.personal .basedetail{
	width: 100%;
}
.personal .basedetail input,.personal .basedetail .baseedits,.personal .basedetail .precent{
	display: inline-block;
	box-sizing: border-box;
	padding-left: 3px;
	width: 214px;
	height: 27px;
	line-height: 27px;
}
.personal .basedetail th{
	padding: 2px 5px;
	height: 50px;
	text-align: left;
}
.personal .basedetail .subhide {
	width: 18px;
	height: 18px;
	background: url(../../images/front_company/save.png);
	padding: 0;
	border: 0;
	vertical-align: bottom;
	cursor: pointer;
	margin-left: 3px;
}
.personal .basedetail .subcancel {
	width: 18px;
	height: 18px;
	background: url(../../images/front_company/subcancel.png);
	padding: 0;
	border: 0;
	vertical-align: bottom;
	cursor: pointer;
}
.personal .basedetail .baseeditinput,.columnthree input.baseeditinput{
	display: none;
}
.personal .column,.personal .columntwo {
	padding-bottom: 20px;
	/*border-bottom: 1px solid #999;*/
}
.personal .columntwo{
	border: none;
}
.personal .columnthree{
	padding-bottom: 20px;
	height: 45px;
}
.personal textarea {
	width: 600px;
	height: 100px;
	vertical-align: middle;
}
.personal .describe{
	margin:0 50px;
}
.personal .cancel {
	display: inline-block;
	height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-left: 50px;
	padding: 0;
}
.personal .addform{
	position: fixed;
	width: 450px;
	left: 50%;
	margin-left: -225px;
	top: 40%;
	z-index: 11;
	background-color: white;
	display: none;
}
.personal .addform td{
	height: 40px;
	padding-left: 30px;
}

.personal .addform table input,.personal .addform table select{
	box-sizing: border-box;
	width: 180px;
}
.personal .addform .columnthree{
	padding-left: 30px;
	padding-top: 10px;
}
#container {
	width: 92%;
	height: 350px;
	margin-left: 15px;
}
.smallinput {
	width: 150px;
}
.bigtd {
	width: 360px;
}
.firsttd{
	padding-left: 50px;
}
.smallimg,.simg{
	width: 18px;
	cursor: pointer;
	margin-left: 10px;
	vertical-align: text-bottom;
}
.red{
	color: darkred;
	margin-right: 5px;
}
.mask{
	position: fixed;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.3);
	top: 0;
	z-index: 10;
	display: none;
}
