#vertify {
	height: 25px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
}
.personal.content {
	min-height: 84%;
	box-sizing: border-box;
}
.personal form{
	margin-left: 100px;
	padding-top: 30px;
}
.personal form label{
	display: inline-block;
	width: 140px;
}
.personal form div {
	margin-bottom: 30px;
	margin-left: 50px;
}
.personal form input {
	padding:2px 10px;
	border: 1px solid lightslategray;
	border-radius: 4px;
}
.personal .next {
	display: inline-block;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-left: 200px;
}
.personal .sure,.personal .cancel {
	display: inline-block;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-left: 50px;
}