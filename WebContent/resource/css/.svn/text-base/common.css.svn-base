html {
	height: 100%;
}
body {
	height: 100%;
	min-width: 1140px;
}
.content{
	width: 1140px;
	margin: 0 auto;
	position: relative;
}

h1 {
	float: left;
	margin: 0;
	width: 120px;
	height: 100%;
	min-height: 54px;
}
h1 a {
	color: lightcoral;
	text-decoration: none;
	display: inline-block;
	height: 100%;
	width: 100%;
	background: url(../images/logo12.png) top center no-repeat;
}
.navbox {
	height: 10%;
	min-height: 92px;
	box-sizing: border-box;
	padding-top: 20px;
}
.nav {
	height: 100%;
	position: relative;
}
.navbar {
	float: right;
	margin: 0;
	width: 85%;
	padding: 0;
	height: 100%;
    min-height: 65px;
    line-height: 100%;
}
.navbar>li {
	float:left;
	font:17px "微软雅黑";
	cursor: pointer;
	border-left: 1px solid #DDDDDD;
	min-height: 40px;
	line-height: 40px;
	text-align: center;
	position: relative;
	width: 12%;
}
.navbar>li:nth-child(1){
	border: none;
}
.navbar>li>ul {
	position: absolute;
	padding: 0;
	display: none;
	background-color: white;
	width: 100%;
	border-radius: 5px;
	z-index: 11;
	font-size: 14px;
}
.navbar>li>ul>li {
	height: 35px;
}
.navbar>li>ul>li a{
	color: #555555;
}
.navbar>li>ul>li:hover a{
	color:#C68E8E;
}
/*注册页面*/
.log,.percenter{
	position: relative;
	top: -10px;
	height: 25px;
	margin: 0 auto;
	position: relative;
}
.percenter .count{
	font-size:12px;
	display: inline-block;
	border-radius: 14px 14px 14px 0;
	background-color: #ff5151;
	height: 14px;
	padding: 2px;
	line-height: 14px;
	color: white;
	position: absolute;
	top: -5px;
	right: -2px;
}
.log a {
	color: black;
	display: inline-block;
	padding: 0 6px;
}
.log a:hover{
	color: #C68E8E;
}
.percenter {
	cursor:default ;
}
.struck {
	min-height: 82%;
	box-sizing: border-box;
}
/*标题和页脚*/
.title,.footer {
	width: 100%;
	height: 8%;
	min-height: 50px;
	line-height: 50px;
	background-color: #00529c;
	color: white;
	font-size: 24px;
	display: table;
}
.title>.content,.footer>.content{
	height: 100%;
	box-sizing: border-box;
	display: table;
}
.title>.content>span {
	display: table-cell;
	vertical-align: middle;
}
.footer{
	font-size: 16px;
}
.footer span {
	display: table-cell;
	vertical-align: middle;
	margin-right: 20px;
	text-align: center;
}
.footer a{
	padding-top: 10px;
	color: white;
	margin: 0 10px;
}
.filetitle{
	font-size: 20px;
	font-weight: 700;
	color: #555;
}
.pop,.poplog{
	position:fixed;
	width:400px;
	height:100px;
	background:white;
	left:50%;
	top:30%;
	margin-left:-200px;
	z-index:20;
	border-radius: 5px;
}
.pop h3,.poplog h3{
	font-size: 16px;
	text-align: center;
}
.pop div{
	text-align: center;
}
.pop a{
	display: inline-block;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin:0  20px;
	
}
.mask {
	position: fixed;
	height: 100%;
	width: 100%;
	top: 0;
	background-color: black;
	opacity: 0.4;
	z-index: 10;
	display: none;
}
