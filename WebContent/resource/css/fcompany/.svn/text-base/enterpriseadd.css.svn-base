.personal {
	min-height: 74%;

	box-sizing: border-box;
}
.personal>div {
	box-sizing: border-box;
	padding: 14px 0 11px;
}
.personal .detail>div,.personal .detail>ul {
	float: left;
}

.personal .detailright{
	width: 800px;
	padding-top: 10px;
	margin-bottom: 20px;
	box-sizing: border-box;
	padding: 15px;
	border-radius: 15px;
}
.personal form{
	margin-left: 50px;
}
.personal form input,.personal form select,.personal input {
	padding:2px 10px;
	border: 1px solid lightslategray;
	border-radius: 4px;
	box-sizing: border-box;
	width: 250px;
}
.personal  label,.personal form>label{
	display: inline-block;
	width: 180px;
	text-align: right;
	padding-right: 50px;
}

.personal .detailright>div,.personal form>div {
	margin-bottom: 25px;
	margin-left: 80px;
}

.personal input.identy {
	display: inline-block;
	height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-left: 280px;
}
.personal .intitle{
	font-size: 20px;
	font-weight: bold;
	height: 50px;
}
.personal .cancel,.personal .files {
	display: inline-block;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-left: 50px;
	padding: 0;
	position: relative;
}
.personal .files input{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	opacity: 0;
}
.personal .files{
	margin: 0;
}
