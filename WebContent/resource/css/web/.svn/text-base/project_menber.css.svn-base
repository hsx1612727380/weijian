@font-face {
	font-family: 'iconfont';
	src: url('../../iconfont/iconfont.eot');
	src: url('../../iconfont/iconfont.eot?#iefix') format('embedded-opentype'), url('../../iconfont/iconfont.woff') format('woff'), url('../../iconfont/iconfont.ttf') format('truetype'), url('../../iconfont/iconfont.svg#iconfont') format('svg');
}

.iconfont {
	font-family: "iconfont" !important;
	font-size: 60px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-webkit-text-stroke-width: 0.2px;
	-moz-osx-font-smoothing: grayscale;
}

.personal {
	min-height: 74%;
}

.comtitle {
	padding: 20px 0 10px;
	width: 100%;
	height: 200px;
}

.comtitle>div {
	height: 120px;
	background-color: white;
	opacity: 0.95;
}

.comtitle .intitle {
	font-size: 30px;
	line-height: 40px;
	margin-bottom: 20px;
	font-weight: bold;
	color: #333333;
}

.comtitle .detailtitle {
	margin: 0 auto;
	padding: 0;
	width: 1200px;
}

.comtitle .detailtitle li {
	float: left;
	text-align: center;
	background: rgb(0, 82, 156);
	color: #E8F4FF;
	width: 100px;
	height: 100px;
	padding: 10px 5px;
	margin-left: 40px;
	cursor: pointer;
	font-size: 16px;
	opacity: 0.8;
}

.comtitle .detailtitle p {
	margin: 0;
}

.comtitle .detailtitle li:hover {
	background: #2FA1A6;
}

.column {
	margin: 10px 0;
	padding: 10px 0;
}

.column .ntitle {
	font-size: 22px;
	font-weight: bold;
	color: #333333;
}

.column .basedetail {
	font-size: 16px;
	padding: 0;
}

.column .basedetail li {
	float: left;
	text-align: center;
	width:260px;
	background-color: #ECECEC;
	margin: 0 12px;
	margin-bottom: 12px;
}

.column .basedetail h3 {
	font-size: 1em;
}
.column .prodetail {
	font-size: 16px;
	padding: 0;
}

.column .prodetail li {
	float: left;
	text-align: center;
	width:350px;
	background-color: #ECECEC;
	margin: 0 15px;
	margin-bottom: 12px;
}
.column .prodetail h2 {
	font-size: 1.1em;
	text-align: left;
	padding-left: 30px;
}
.column .prodetail h3 {
	font-size: 1em;
}
.column table {
	font-size: 16px;
	width: 98%;
	text-align: center;
	margin: 16px 0;
}

.column table th {
	background-color: #4488FF;
	color: white;
	padding: 10px 5px;
}

.column table td {
	padding: 10px 5px;
}

.column>p {
	height: 50px;
	overflow: auto;
}

.pointer {
	cursor: pointer;
}

.pointer:hover {
	background: #2FA1A6;
	opacity: 0.8;
	color: white;
}

.mask {
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	background-color: rgba(0, 0, 0, 0.2);
	z-index: 100;
	display: none;
}

.meninfo {
	position: fixed;
	top: 20%;
	padding: 10px;
	width: 800px;
	background-color: white;
	z-index: 200;
	left: 50%;
	margin-left: -400px;
	height: 500px;
	overflow: auto;
	display: none;
}
.meninfo table{
	width: 100%;
}
.meninfo .bgg{
	background-color: #4488FF;
	color: white;
}
.meninfo th {
	text-align: left;
	height: 40px;
	font-size: 24px;
	font-weight: 100;
}

.meninfo td {
	text-align: center;
	height: 30px;
}
.fcolor {
	color: #4488FF;
}
.X {
	position: absolute;
	right: 10px;
	top: 5px;
	cursor: pointer;
	font-size: 20px;
	font-family: '宋体';
	opacity: 0.6;
}
.X:hover{
	opacity: 1;
}
#container{
	width: 543px;
	height: 396px;
	margin: 0 17px 12px 0;
	float: right;
}
