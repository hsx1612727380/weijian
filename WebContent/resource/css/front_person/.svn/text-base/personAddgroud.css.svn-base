.personal {
	min-height: 74%;

	box-sizing: border-box;
}
.personal>div {
	box-sizing: border-box;
	padding: 14px 0 11px;
}
.personal .autstate {
	width: 100%;
	border-bottom: 1px solid #C0C0C0;
	font:bold 18px sans-serif;
}
.personal .detail>div,.personal .detail>ul {
	float: left;
}
.personal .detailbar {
	padding: 0;
	border-radius: 10px;
	width: 170px;
	margin: 0 0 0 30px;
}
.personal .detailbar>li{
	box-sizing: border-box;
	height: 40px;
	line-height: 40px;
	background-color: #E7F1F9;
	padding-left: 20px;
}
.personal .detailbar>li a{
	display: inline-block;
	width: 100%;
	color: #555555;
}
.personal .detailbar>li a:hover{
	color: #0088E6;
}
.personal .detailright{
	width: 800px;
	margin-left: 95px;
	padding-top: 10px;
	margin-bottom: 20px;
	background-color: #E7F1F9;
	box-sizing: border-box;
	padding: 15px;
	border-radius: 15px;
	font-size: 14px;
}
.personal .detailright input,.personal .detailright select {
	height: 20px;
	width: 115px;
	box-sizing: border-box;
	border-radius: 5px;
	border: 1px solid lightslategray;
}
.personal .detailright table{
	width: 100%;
}
.personal .detailright tr {
	height: 30px;
}
.personal .detailright input {
	box-sizing: border-box;
	padding: 2px 4px;
}
.detailright .derighttop div{
	float: left;
	margin-left: 20px;
	margin-top: 15px;
	height: 25px;
	line-height: 25px;
	text-align: center;
	width: 82px;
	border: none;
	border-radius: 3px;
	background-color: #3399FF;
	color: white;
	cursor: pointer;
	font-size: 14px;
	margin-bottom: 20px;
}

.addground td {
	text-align: center;
	background-color: #fff;
}
.personal .addground{
	width: 100%;
	background-color: #999;
}
.addground td span {
	display: inline-block;
	height: 20px;
    line-height: 20px;
    text-align: center;
    width: 50px;
    border: none;
    border-radius: 3px;
    background-color: #3399FF;
    color: white;
    cursor: pointer;
    font-size: 14px;
    margin:0 2px;
}
.personal .persontitle .filetile{
	font-size: 20px;
	font-weight: 700;
	color: #555;
}
.personal .apply>div{
	float: left;
	border: 1px solid #D9D9D9;
	width: 100px;
	height: 30px;
	text-align: center;
	line-height: 30px;
	margin-top: 20px;
	border-radius: 5px;
	margin-left: 5px;
	background-color: white;
	cursor: pointer;
	margin-bottom: 20px;
}
.personal .apply>div.active{
	background-color: #3399FF;
	color: white;
}
.personal .apply table{
	background-color: #999;
}
.personal .apply td{
	text-align: center;
	background-color: white;
}
.selected{
	background-color: rgba(54, 146, 223,0.7);
}
.selected a{
	color: white;
}