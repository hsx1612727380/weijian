<?xml version="1.0" encoding="UTF-8"?>
<excel id="recordSalary" code="recordSalary" name="进退场记录导入">
    <colgroup>
        <col index="A" width='17em'></col>
        <col index="B" width="17em"></col>
        <col index="C" width='20em'></col>
        <col index="D" width='13em'></col>
    </colgroup>
    <title>
        <tr height="16px">
            <td rowspan="1" colspan="4" value="进退场记录导入" />
        </tr>
    </title>
    <thead>
        <tr height="16px">
        	<th value="工人手机号" />
        	<th value="班组名称" />
            <th value="打卡时间" />
            <th value="进退场" />
        </tr>
    </thead>
    <tbody>
        <tr height="16px" firstrow="2" firstcol="0" repeat="100">
            <td type="string" isnullable="false" maxlength="11" />
            <td type="string" isnullable="false" maxlength="40" />
            <td type="date" isnullable="false" format="yyyy-mm-dd HH:MM:SS" maxlength="50" />
            <td type="enum" isnullable="true" format="进场,退场" />
        </tr>
    </tbody>
</excel>